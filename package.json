{
	"name": "libopus-node",
	"version": "0.0.0",
	"description": "libopus bindings for Node",
	"main": "lib/index.js",
	"types": "typings/index.d.ts",
	"license": "MIT",
	"scripts": {
		"install": "node-gyp-build",
		"build": "node-gyp-build",
		"prebuild": "prebuildify --napi --strip --tag-libc",
		"lint": "prettier --check . && eslint lib --format=pretty",
		"lint:fix": "prettier --write . && eslint lib --fix --format=pretty",
		"test": "node tests/test.js"
	},
	"keywords": [
		"native",
		"opus",
		"module",
		"c",
		"c++",
		"bindings",
		"binary"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/biw/libopus-node.git"
	},
	"dependencies": {
		"node-addon-api": "8.3.1",
		"node-gyp-build": "^4.8.0"
	},
	"devDependencies": {
		"@types/node": "22.15.21",
		"eslint": "9.27.0",
		"eslint-config-neon": "0.2.7",
		"eslint-formatter-pretty": "6.0.1",
		"node-gyp": "11.2.0",
		"prebuildify": "^6.0.1",
		"prettier": "3.5.3",
		"typescript": "5.8.3"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"packageManager": "yarn@4.9.1"
}

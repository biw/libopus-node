{
  "name": "libopus-node",
  "version": "0.0.1",
  "description": "libopus bindings for Node",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "install": "node-gyp-build",
    "build-native": "node-gyp-build",
    "build-tsup": "tsup",
    "build": "npm run build-native && npm run build-tsup",
    "prebuild": "prebuildify --napi --strip --tag-libc",
    "lint": "tsc --noEmit && eslint .",
    "test": "node src/tests/test.js"
  },
  "keywords": [
    "native",
    "opus",
    "libopus",
    "module",
    "c",
    "c++",
    "bindings",
    "binary"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/biw/libopus-node.git"
  },
  "dependencies": {
    "node-addon-api": "8.3.1",
    "node-gyp-build": "^4.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/babel__core": "^7.20.5",
    "@types/node": "22.18.0",
    "electron": "^39.1.0",
    "eslint": "9.39.1",
    "node-gyp": "11.2.0",
    "prebuildify": "^6.0.1",
    "tsup": "8.5.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@4.11.0"
}

{
	"name": "libopus",
	"version": "0.10.0",
	"description": "Opus bindings for Node",
	"main": "lib/index.js",
	"types": "typings/index.d.ts",
	"license": "MIT",
	"scripts": {
		"prepare": "node-pre-gyp install --fallback-to-build",
		"build": "node-pre-gyp install build package",
		"lint": "prettier --check . && eslint lib --format=pretty",
		"lint:fix": "prettier --write . && eslint lib --fix --format=pretty",
		"test": "node tests/test.js"
	},
	"keywords": [
		"native",
		"opus",
		"module",
		"c",
		"c++",
		"bindings",
		"binary"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/biw/libopus-js.git"
	},
	"dependencies": {
		"@mapbox/node-pre-gyp": "2.0.0",
		"node-addon-api": "8.3.1"
	},
	"devDependencies": {
		"@types/node": "22.15.21",
		"eslint": "9.27.0",
		"eslint-config-neon": "0.2.7",
		"eslint-formatter-pretty": "6.0.1",
		"node-gyp": "11.2.0",
		"prettier": "3.5.3",
		"typescript": "5.8.3"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"binary": {
		"module_name": "opus",
		"module_path": "./prebuild/{node_abi}-napi-v{napi_build_version}-{platform}-{arch}-{libc}-{libc_version}/",
		"remote_path": "v{version}",
		"package_name": "{module_name}-v{version}-{node_abi}-napi-v{napi_build_version}-{platform}-{arch}-{libc}-{libc_version}.tar.gz",
		"host": "https://github.com/biw/libopus-js/releases/download/",
		"napi_versions": [
			3
		]
	},
	"packageManager": "yarn@4.9.1"
}
